 */

bench {
   transitions {
      model {
         t1  : s1 > s2;
         t2  : s2 > s1;
         t3  : s1 > s3;
         t4  : s3 > s1;
         t5  : s1 > s4;
         t6  : s4 > s1;
         t7  : s2 > s3;
         t8  : s3 > s2;
         t9  : s3 > s4;
         t10 : s4 > s3;
         t11 : s2 > s4;
         t12 : s4 > s2;
      }
   }
   states {
      s1 @init {}
   }
}

